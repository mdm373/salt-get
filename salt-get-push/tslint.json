{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console" : false,
        "max-line-length": [true, 140],
        "interface-name": [false, "always-prefix"],
        "object-literal-sort-keys": false,
        "ordered-imports" : false,
        "quotemark" : [true, "single"],
        "semicolon": [true, "never"]
    },
    "rulesDirectory": []
}